>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>


SQL> DESC ORDER

 Name                                      Null?    Type
 ----------------------------------------- -------- ----------------------
 ORDER_ID                                  NOT NULL VARCHAR2(20)
 DATE_OF_ORDER                                      DATE
 TOTAL_PRICE                                        FLOAT(25)
 TRANSACTION_ID                                     VARCHAR2(30)
 CUS_ID                                             NUMBER(38)
 PARTNER_ID                                         VARCHAR2(25)


SQL> DESC DELIVERY_PARTNER;

 Name                                      Null?    Type
 ----------------------------------------- -------- --------------------
 PARTNER_ID                                NOT NULL VARCHAR2(25)
 PARTNER_NAME                                       VARCHAR2(20)
 MOBILE_NO                                          NUMBER(10)


SQL> DESC PAYMENT;

 Name                                      Null?    Type
 ----------------------------------------- -------- --------------------
 TRANSACTION_ID                            NOT NULL VARCHAR2(30)
 DATE_OF_PAYMENT                                    DATE
 AMOUNT                                             FLOAT(20)


SQL> DESC HANDLE;

 Name                                      Null?    Type
 ----------------------------------------- -------- --------------------
 CUS_ID                                             NUMBER(38)
 EMP_ID                                             NUMBER(38)


SQL> DESC HAS;

 Name                                      Null?    Type
 ----------------------------------------- -------- --------------------
 ORDER_ID                                           VARCHAR2(20)
 PRODUCT_ID                                         NUMBER(38)


